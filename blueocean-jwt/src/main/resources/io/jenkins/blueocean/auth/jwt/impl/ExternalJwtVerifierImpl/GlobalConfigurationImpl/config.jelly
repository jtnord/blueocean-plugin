<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:section title="${%External JWT Verifier}">
        <f:entry field="ssoUri" title="OpenID Connect Server URL">
            <f:textbox/>
        </f:entry>
        <f:entry field="audience" title="Allowed Audience(s)">
            <!-- workaround JENKINS-27901 -->
            <f:textarea value="${instance.joinWithNewLines(instance.getAudience())}"/>
        </f:entry>
    </f:section>
</j:jelly>
